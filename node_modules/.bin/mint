#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Documents/GitHub/docs/node_modules/.pnpm/mintlify@4.2.182_@radix-ui+react-popover@1.1.15_@types+react@19.2.2_react-dom@18.3.1_re_c117243fd0fc437a591e5936b5821ced/node_modules/mintlify/node_modules:/Users/mac/Documents/GitHub/docs/node_modules/.pnpm/mintlify@4.2.182_@radix-ui+react-popover@1.1.15_@types+react@19.2.2_react-dom@18.3.1_re_c117243fd0fc437a591e5936b5821ced/node_modules:/Users/mac/Documents/GitHub/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Documents/GitHub/docs/node_modules/.pnpm/mintlify@4.2.182_@radix-ui+react-popover@1.1.15_@types+react@19.2.2_react-dom@18.3.1_re_c117243fd0fc437a591e5936b5821ced/node_modules/mintlify/node_modules:/Users/mac/Documents/GitHub/docs/node_modules/.pnpm/mintlify@4.2.182_@radix-ui+react-popover@1.1.15_@types+react@19.2.2_react-dom@18.3.1_re_c117243fd0fc437a591e5936b5821ced/node_modules:/Users/mac/Documents/GitHub/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/mintlify@4.2.182_@radix-ui+react-popover@1.1.15_@types+react@19.2.2_react-dom@18.3.1_re_c117243fd0fc437a591e5936b5821ced/node_modules/mintlify/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/mintlify@4.2.182_@radix-ui+react-popover@1.1.15_@types+react@19.2.2_react-dom@18.3.1_re_c117243fd0fc437a591e5936b5821ced/node_modules/mintlify/index.js" "$@"
fi
